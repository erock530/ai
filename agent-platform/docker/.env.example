# PostgreSQL Configuration
POSTGRES_USER=n8n
POSTGRES_PASSWORD=your_secure_password
POSTGRES_DB=n8n

# N8N Configuration
N8N_ENCRYPTION_KEY=your_secure_encryption_key
N8N_USER_MANAGEMENT_JWT_SECRET=your_secure_jwt_secret

# Resource Limits (optional, can be adjusted in docker-compose.yml)
N8N_MEMORY_LIMIT=4G
POSTGRES_MEMORY_LIMIT=2G
OLLAMA_MEMORY_LIMIT=4G
QDRANT_MEMORY_LIMIT=2G

# Host Configuration
HOST_DOMAIN=your_domain.com
